            </ul>
         </section>
         <!-- /.sidebar -->
      </aside>
      <!-- Content Wrapper. Contains page content -->
      <div class="content-wrapper">
         <!-- Main content -->
         <section class="content">
            <div class="row">
               <div class="col-xs-12">
                  <div class="box">
                     <div class="box-header  ">
                        <div  class="pull-left ">
                           <img src= {{vendorlogo}} >
                        </div>
                        <div  class="pull-right">
                           <h1 class="box-title"><b>{{vendorname}}</b></h1>
                           <br>
                           <small>
                              <h2 class="box-title">Phone: {{vendorPhone}}</h2>
                           </small>
                        </div>
                     </div>

                     <div class="profiles">
                        <ul>
                        <li ng-repeat="prof in profilelist">
                           <div class="box box-solid ">
                              <div class="box-header with-border">
                                 <!-- <h3 class="box-title">{{prof.id}}</h3> -->
                                 <h1 class="box-title"><b>Id    :    {{prof.id}}</b></h1>
                                 <div class="box-tools pull-right">
                                    <!-- Buttons, labels, and many other things can be placed here! -->
                                    <!-- Here is a label for example -->
                                    <button type="submit" class="btn btn-primary pull-right" ng-click= "viewProfileDetail($index)">View Details</button>
                                 </div>
                                 <!-- /.box-tools -->
                              </div>
                              <!-- /.box-header -->
                              <div class="box-body">
                                 <div class="wrapper">
                                    <div class="banner">
                                       <img src={{prof.logo}}>
                                    </div>
                                    <div class="row">
                                       <div class="col-xs-5">
                                          <div class="table-responsive">
                                             <table class="table">
                                                <tr>
                                                   <th style="width:50%">Name:</th>
                                                   <td>{{prof.name}}</td>
                                                </tr>
                                                <tr>
                                                   <th>Gender:</th>
                                                   <td>{{prof.gender}}</td>
                                                </tr>
                                                <tr>
                                                   <th>Age:</th>
                                                   <td>{{prof.age}}</td>
                                                </tr>
                                                <tr>
                                                   <th>Mother Tongue:</th>
                                                   <td>{{prof.mothertongue}}</td>
                                                </tr>
                                             </table>
                                          </div>
                                       </div>
                                       <div class="col-xs-5">
                                          <div class="table-responsive">
                                             <table class="table">
                                                <tr>
                                                   <th style="width:50%">Cast:</th>
                                                   <td>{{prof.community}}
                                                      <span ng-if="prof.castdetails">
                                                      ( {{prof.castdetails}} )
                                                      </span>
                                                   </td>
                                                </tr>
                                                <tr>
                                                   <th>Occupation:</th>
                                                   <td>{{prof.occupation}}
                                                      <span ng-if="prof.occupationdetails">
                                                      ( {{prof.occupationdetails}} )
                                                      </span>
                                                   </td>
                                                </tr>
                                                <tr>
                                                   <th>Education:</th>
                                                   <td>{{prof.education}}
                                                      <span ng-if="prof.educationdetails">
                                                      ( {{prof.educationdetails}} )
                                                      </span>
                                                   </td>
                                                </tr>
                                                <tr>
                                                   <th>Phone:</th>
                                                   <td>{{prof.phone}}</td>
                                                </tr>
                                             </table>
                                          </div>
                                       </div>
                                    </div>
                                    <!-- /.col -->
                                 </div>
                                 <!--End of Profiles-->
                              </div>
                           </div>
                     </div>
                     <!-- /.box -->
                  </div>
                  <!-- /.col -->
               </div>
               <!-- /.row -->
