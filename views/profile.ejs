                  <p class="text-muted">
                    {{profile.gender}}
                  </p>

                  <hr>
               <strong><i class="fa fa-book margin-r-5"></i>  Income</strong>
                  <p class="text-muted">
                    {{profile.income}}
                  </p>

                  <hr>
              <strong><i class="fa fa-book margin-r-5"></i>  Father </strong>
                  <p class="text-muted">
                    {{profile.father.name}}
                  </p>
                  <p class="text-muted">
                    {{profile.father.occupation}}
                  </p>
                  <hr>
                  
                <strong><i class="fa fa-book margin-r-5"></i>  Mother</strong>
                  <p class="text-muted">
                    {{profile.mother.name}}
                    {{profile.mother.occupation}}
                  </p>

                  <hr>   
                  <strong><i class="fa fa-file-text-o margin-r-5"></i> Summary</strong>
                  <p>{{profile.summary}}</p>
                  <hr>   
                
                </div><!-- /.box-body -->
              </div><!-- /.box -->

                </div><!-- /.tab-content -->
