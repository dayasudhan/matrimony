                                                                   <select ng-model="gender_select" ng-change="updateSelectGender (1)">
      <option ng-repeat="i in genders" value="{{ i.name }}" ng-selelected="{{ gender_select == i.name }}">{{ i.name }}</option>
    </select>
                                                </div>
                                            </form>
                                        </li>
                                        <li>
                                            <form>
                                                <div class="form-group">
                                                 <select ng-model="cast_select" ng-change="updateSelectCast(1)">
                                                      <option ng-repeat="i in communitys" value="{{ i.name }}" ng-selelected="{{ cast_select == i.name }}">{{ i.name }}</option>
    </select>                                                 
                                                </div>
                                            </form>
                                        </li>
                                        <li>
                                            <form>
                                                <div class="form-group">
                                                    <select class="form-control" ng-model="age_select" ng-change="updateSelectAge(1)">
