                  <p class="text-muted text-center">{{profile.community}}</p>
                  <p class="text-muted text-center">{{profile.age}}</p>
                      
                  <button type="submit" class="btn btn-info pull-right" ng-click= "nextProfile()">Next</button>
                  

                  <button type="submit" class="btn btn-info pull-left" ng-click= "previousProfile()">Previous</button>

                  
                </div><!-- /.box-body -->
              </div><!-- /.box -->

             
            </div><!-- /.col -->
            
<div class="col-md-6">
